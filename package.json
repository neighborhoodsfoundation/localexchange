{
  "name": "localex",
  "version": "1.0.0",
  "description": "LocalEx - Local Trading Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "ts-node scripts/migrate.ts migrate",
    "db:rollback": "ts-node scripts/migrate.ts rollback",
    "db:status": "ts-node scripts/migrate.ts status",
    "db:seed": "ts-node scripts/seed.ts",
    "db:reset": "ts-node scripts/reset-db.ts",
    "redis:test": "ts-node scripts/test-redis-services.ts",
    "redis:install": "powershell -ExecutionPolicy Bypass -File scripts/install-redis.ps1",
    "redis:start": "powershell -ExecutionPolicy Bypass -File scripts/start-redis.ps1",
    "redis:start:with-commander": "powershell -ExecutionPolicy Bypass -File scripts/start-redis.ps1 -WithCommander",
    "search:test": "ts-node scripts/test-search-services.ts",
    "search:install": "powershell -ExecutionPolicy Bypass -File scripts/install-opensearch.ps1",
    "search:start": "docker-compose -f docker-compose.opensearch.yml up -d",
    "search:stop": "docker-compose -f docker-compose.opensearch.yml down",
    "search:setup": "ts-node scripts/setup-search-indices.ts",
    "storage:start": "docker-compose -f docker-compose.localstack.yml up -d",
    "storage:stop": "docker-compose -f docker-compose.localstack.yml down",
    "storage:setup": "ts-node scripts/setup-s3-bucket.ts",
    "storage:test": "ts-node scripts/test-storage.ts",
    "test:coverage-audit": "ts-node scripts/test-coverage-audit.ts",
    "test:integration": "ts-node scripts/run-integration-tests.ts",
    "test:performance": "ts-node scripts/performance-benchmark.ts",
    "test:e2e": "ts-node scripts/test-e2e.ts",
    "test:load": "jest tests/integration/load-testing.test.ts --verbose --detectOpenHandles --forceExit",
    "test:security": "jest tests/integration/security-audit.test.ts --verbose --detectOpenHandles --forceExit",
    "test:beta": "jest tests/integration/beta-testing.test.ts --verbose --detectOpenHandles --forceExit",
    "test:all": "npm run test:coverage-audit && npm run test:integration && npm run test:performance && npm run test:e2e",
    "sync": "powershell -ExecutionPolicy Bypass -File sync-repo.ps1"
  },
  "keywords": [
    "local",
    "trading",
    "marketplace",
    "peer-to-peer",
    "credits"
  ],
  "author": "Neighborhoods Foundation",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.906.0",
    "@aws-sdk/client-s3": "^3.906.0",
    "@aws-sdk/lib-storage": "^3.906.0",
    "@aws-sdk/s3-request-presigner": "^3.906.0",
    "@opensearch-project/opensearch": "^2.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "^0.33.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/sharp": "^0.32.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supabase": "^2.54.11",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
